<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qyy.jyshop.dao.SpellDeliveryDao">
    <select id="selectLogiByOrderId" resultType="com.qyy.jyshop.model.SpellDelivery" >
        select *
        from shop_spell_delivery a
        where a.order_id = #{orderId}
    </select>
</mapper>